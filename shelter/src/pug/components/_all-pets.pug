// Pets list 

include ../mixins/_pagination
include ../mixins/_pet-card

section.all-pets 
    .container
        h1.h2.all-pets__title Our friends who<br> are looking for a house
        .all-pets__list 
            each pet in [1,2,3,4,5,6,7,8]
                +pet-card(pet, 'all-pets__pet-card', true)
        +pagination()